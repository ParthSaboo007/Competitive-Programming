int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
	int t;
	cin>>t;
	while(t--)
    {
        string s;
        string arr[5];
        for(int i=0;i<5;i++)
            {cin>>s;
        arr[i]=s;}
        for(int i=0;i<5;i++)
            cout<<arr[i]<<"  ";

    }
	return 0;
}
